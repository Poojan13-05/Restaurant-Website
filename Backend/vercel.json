{
    "version": 2,
    "builds": [
        {
            "src": "routes/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/auth(.*)",
            "dest": "/routes/auth.js"
        },
        {
            "src": "/api/order(.*)",
            "dest": "/routes/order.js"
        },
        {
            "src": "/api/menuitem(.*)",
            "dest": "/routes/menuitem.js"
        },
        {
            "src": "/api/reservation(.*)",
            "dest": "/routes/reservation.js"
        }
    ]
}
